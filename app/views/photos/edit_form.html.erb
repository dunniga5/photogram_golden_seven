<h1>Edit Photo</h1>

<form action="/update_photo/<%= @photo.id %>">
  <div>
    <label for="photo_caption">Caption:</label>
    <input id="photo_caption" type="text" name="caption" value="<%= @photo.caption %>">
  </div>
  <div>
    <label for="photo_image_url">Image URL:</label>
    <input id="photo_image_url" type="text" name="source" value="<%= @photo.source %>">
  </div>
  <div>
    <button>Update Photo</button>
  </div>
</form>
